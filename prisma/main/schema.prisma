generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/.prisma/main-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Instansi {
  id             Int     @id @default(autoincrement())
  id_kementerian Int?    @db.Integer
  nama           String? @db.VarChar(255)

  Kementerian Kementerian? @relation("MasterInstansiKementerian", fields: [id_kementerian], references: [id])

  @@map("m_instansi")
}

model Kementerian {
  id   Int     @id @default(autoincrement())
  nama String? @db.VarChar(255)

  Instansi Instansi[] @relation("MasterInstansiKementerian")

  @@map("m_kementerian")
}

